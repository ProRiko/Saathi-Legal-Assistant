# Environment variables for Saathi Legal Assistant
# Copy this file to .env and update the values

# OpenRouter API Key
# Get this from https://openrouter.ai/
OPENROUTER_API_KEY=your-actual-api-key-here

# API Configuration
DEFAULT_MODEL=mistralai/mistral-7b-instruct:free
API_TIMEOUT=30
MAX_TOKENS=500
TEMPERATURE=0.7

# Flask Configuration
FLASK_DEBUG=True
FLASK_HOST=127.0.0.1
FLASK_PORT=5000

# Application Settings
APP_NAME=Saathi Legal Assistant
APP_VERSION=1.0.0
